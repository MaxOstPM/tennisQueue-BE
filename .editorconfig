root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.swift]
indent_style = space
indent_size = 4
max_line_length = 130

[*.{yml,yaml}]
indent_style = space
indent_size = 2

[Podfile]
indent_style = space
indent_size = 2
